{
  "name": "carshop",
  "version": "0.0.1",
  "description": "Business Application",
  "dependencies": {
    "@sap/audit-logging": "5.6.3",
    "@sap/cds": "6.4.0",
    "@sap/cds-common-content": "1.0.1",
    "@sap/cds-dk": "6.4.0",
    "@sap/xsenv": "3.4.0",
    "@sap/xssec": "^3.2.15",
    "debug": "4.3.4",
    "express": "4.18.2",
    "hdb": "^0.19.0",
    "passport": "^0.6.0",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.1.4",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21"
  },
  "scripts": {
    "start": "npx cds run",
    "undeploy": "cf undeploy carshop --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-managecars": "cds watch --open managecars/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-createorder": "cds watch --open createorder/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[production]": {
          "strategy": "jwt"
        },
        "[development]": {
          "strategy": "mock"
        },
        "kind": "jwt"
      },
      "db": {
        "kind": "sql",
        "credentials": {
          "database": "db.sqlite"
        }
      },
      "features": {
        "in_memory_db": true
      },
      "approuter": {
        "kind": "cloudfoundry"
      },
      "[hybrid]": {
        "auth": {
          "kind": "xsuaa"
        }
      },
      "[production]": {
        "db": {
          "kind": "hana-cloud"
        }
      }
    }
  },
  "sapux": [
    "app/admin",
    "app/browse",
    "app/createorder",
    "app/managecars",
    "adminflpect1"
  ],
  "devDependencies": {
    "@sap/ux-specification": "UI5-1.108",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.1.4"
  }
}